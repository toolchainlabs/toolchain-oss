        {
            "source": "^api/v1/repos/(?P<customer_slug>[^/.]+)/(?P<repo_slug>[^/.]+)/builds/(?P<run_id>[^/.]+)/workunits/$",
            "target": "/api/v1/repos/{customer_slug}/{repo_slug}/builds/{run_id}/workunits/",
            "pass_args": [
                "customer_slug",
                "repo_slug",
                "run_id"
            ],
            "timeout": 10,
            "service": "buildsense/api",
            "name": "builds-get-workunits-data"
        },
        {
            "source": "^api/v1/repos/(?P<customer_slug>[^/.]+)/(?P<repo_slug>[^/.]+)/builds/(?P<run_id>[^/.]+)/artifacts/(?P<artifact_id>[^/]+)/$",
            "target": "/api/v1/repos/{customer_slug}/{repo_slug}/builds/{run_id}/artifacts/{artifact_id}/",
            "pass_args": [
                "customer_slug",
                "repo_slug",
                "run_id",
                "artifact_id"
            ],
            "service": "buildsense/api",
            "name": "artifacts"
        }
    ]
}