            "source": "^api/v1/repos/(?P<customer_slug>[^/.]+)/(?P<repo_slug>[^/.]+)/builds/(?P<run_id>[^/.]+)/trace/$",
            "target": "/api/v1/repos/{customer_slug}/{repo_slug}/builds/{run_id}/trace/",
            "pass_args": [
                "customer_slug",
                "repo_slug",
                "run_id"
            ],
            "service": "buildsense/api",
            "name": "builds-get-trace-data",
            "timeout": 10
        },
        {
            "source": "^api/v1/repos/(?P<customer_slug>[^/.]+)/(?P<repo_slug>[^/.]+)/builds/(?P<run_id>[^/.]+)/raw/$",
            "target": "/api/v1/repos/{customer_slug}/{repo_slug}/builds/{run_id}/raw/",
            "pass_args": [
                "customer_slug",
                "repo_slug",
                "run_id"
            ],
            "timeout": 10,
            "service": "buildsense/api",
            "name": "builds-get-raw-data"
