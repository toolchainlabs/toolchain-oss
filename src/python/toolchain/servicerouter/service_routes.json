            "target": "/api/v1/repos/{customer_slug}/{repo_slug}/buildsense/batch/",
            "pass_args": [
                "customer_slug",
                "repo_slug"
            ],
            "service": "buildsense/api",
            "timeout": 15,
            "name": "buildsense-batch"
        },
        {
            "source": "^api/v1/repos/(?P<customer_slug>[^/.]+)/(?P<repo_slug>[^/.]+)/buildsense/$",
            "target": "/api/v1/repos/{customer_slug}/{repo_slug}/buildsense/",
            "pass_args": [
                "customer_slug",
                "repo_slug"
            ],
            "service": "buildsense/api",
            "name": "buildsense-config"
        },
        {
            "source": "^api/v1/repos/(?P<customer_slug>[^/.]+)/(?P<repo_slug>[^/.]+)/builds/(?P<run_id>[^/.]+)/$",
            "target": "/api/v1/repos/{customer_slug}/{repo_slug}/builds/{run_id}/",
            "pass_args": [
                "customer_slug",
                "repo_slug",
                "run_id"
            ],
            "service": "buildsense/api",
            "name": "builds-detail"
        },
