            "target": "/api/v1/repos/{customer_slug}/{repo_slug}/buildsense/{run_id}/workunits/",
            "pass_args": [
                "customer_slug",
                "repo_slug",
                "run_id"
            ],
            "timeout": 10,
            "service": "buildsense/api",
            "name": "buildsense-workunits-ingest"
        },
        {
            "source": "^api/v1/repos/(?P<customer_slug>[^/.]+)/(?P<repo_slug>[^/.]+)/buildsense/(?P<run_id>[^/.]+)/artifacts/$",
