            "pass_args": [
                "customer_slug",
                "repo_slug"
            ],
            "service": "buildsense/api",
            "name": "builds-list"
        },
        {
            "source": "^api/v1/repos/(?P<customer_slug>[^/.]+)/(?P<repo_slug>[^/.]+)/builds/indicators/$",
            "target": "/api/v1/repos/{customer_slug}/{repo_slug}/builds/indicators/",
            "pass_args": [
                "customer_slug",
                "repo_slug"
            ],
            "service": "buildsense/api",
            "name": "builds-indicators"
        },
        {
