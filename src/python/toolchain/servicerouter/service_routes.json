        {
            "source": "^api/v1/customers/(?P<customer_slug>[^/.]+)/billing/$",
            "target": "/api/v1/customers/{customer_slug}/billing/",
            "pass_args": [
                "customer_slug"
            ],
            "service": "users/api",
            "name": "customer-billing"
        },
        {
            "source": "^api/v1/customers/(?P<customer_slug>[^/.]+)/workers/tokens/$",
            "target": "/api/v1/customers/{customer_slug}/workers/tokens/",
            "pass_args": [
                "customer_slug"
            ],
            "service": "users/api",
            "name": "customer-worker-tokens-view"
