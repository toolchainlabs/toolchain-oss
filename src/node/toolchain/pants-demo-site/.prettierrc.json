{
    "printWidth": 80,
    "useTabs": false,
    "semi": true,
    "singleQuote": true,
    "quoteProps": "as-needed",
    "jsxSingleQuote": false,
    "trailingComma": "es5",
    "bracketSpacing": true,
    "arrowParens": "avoid",
    "parser": "typescript",
    "requirePragma": false,
    "insertPragma": false,
    "endOfLine": "lf",
    "tabWidth": 2,
    "bracketSameLine": false
}
