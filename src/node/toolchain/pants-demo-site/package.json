{
  "name": "demo-site",
  "version": "0.1.0",
  "private": true,
  "workspaces": {
    "nohoist": [
      "**"
    ]
  },
  "dependencies": {
    "@emotion/react": "^11.11.0",
    "@emotion/styled": "^11.11.0",
    "@mui/icons-material": "^5.11.16",
    "@mui/material": "^5.13.1",
    "@reduxjs/toolkit": "^1.9.5",
    "@sentry/react": "^7.52.1",
    "@sentry/tracing": "^7.52.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-force-graph-2d": "^1.25.0",
    "react-mailchimp-subscribe": "^2.1.3",
    "react-redux": "^8.0.5",
    "react-scripts": "5.0.1",
    "typescript": "^5.0.4"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-ci": "react-scripts test --watchAll=false --coverage",
    "eject": "react-scripts eject",
    "lint": "eslint --ext .ts,.js,.tsx src",
    "prettier": "prettier --write './**/*.{ts,tsx}' --config ./.prettierrc.json",
    "prettier-check": "prettier --check  \"{src,tests,cypress}/**/*.{ts,tsx,js}\"",
    "cypress-test": "yarn cypress run"
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "jest": {
    "collectCoverageFrom": [
      "src/**/*.{js,jsx,ts,tsx}",
      "!<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "clover"
    ]
  },
  "devDependencies": {
    "@babel/plugin-proposal-private-property-in-object": "^7.21.0",
    "@cypress/react": "^7.0.3",
    "@cypress/webpack-dev-server": "^3.4.1",
    "@sentry/types": "^7.52.1",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/react": "^14.0.0",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.11",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@types/react-mailchimp-subscribe": "^2.1.1",
    "@types/react-redux": "^7.1.25",
    "@typescript-eslint/eslint-plugin": "^5.59.0",
    "@typescript-eslint/parser": "^5.59.0",
    "cypress": "^12.10.0",
    "cypress-real-events": "^1.7.6",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.32.2",
    "html-webpack-plugin": "5.5.1",
    "prettier": "^2.8.8"
  },
  "proxy": "http://localhost:9050"
}
