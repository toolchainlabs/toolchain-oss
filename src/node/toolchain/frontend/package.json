    "lint": "eslint --ext .ts,.js,.tsx src",
    "build": "webpack --config ./webpack.config.prod.dist.js",
    "dev": "webpack serve --progress --hot --config ./webpack.config.dev.js",
    "start": "webpack serve --progress --config ./webpack.config.dev.live.js",
    "prettier-check": "prettier --check  \"{src,tests,cypress}/**/*.{ts,tsx,js}\"",
    "prettier": "prettier --list-different --write \"{src,tests,cypress}/**/*.{ts,tsx,js}\""
